{
    "version": 2,
    "builds": [
        {
            "src": "BetterGlauco/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "BetterGlauco/wsgi.py"           
        }
    ]
}
