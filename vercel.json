{
    "version": 2,
    "name": "glauco",
    "builds": [
        {
            "src": "BetterGlauco/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb"}
        },
        {
        "src": "build_files.sh",
        "use": "@vercel/static",
        "config": {
          "distDir": "static"
          }
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "BetterGlauco/wsgi.py"
        }
      ]
}
